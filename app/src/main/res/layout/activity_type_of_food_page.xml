<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TypeOfFoodPage"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="@dimen/_140sdp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="@dimen/_40sdp"
        android:layout_marginLeft="@dimen/_5sdp">

        <androidx.cardview.widget.CardView
            android:id="@+id/Card_DesRes"
            android:layout_width="@dimen/_140sdp"
            android:layout_height="@dimen/_150sdp"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            app:cardCornerRadius="@dimen/_10sdp"
            app:cardElevation="@dimen/_5sdp"
            android:background="#fff">

            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_75sdp"
                app:cardElevation="@dimen/_5sdp"
                app:cardCornerRadius="@dimen/_5sdp"
                android:layout_gravity="center|top"
                android:layout_marginTop="@dimen/_10sdp">

                <ImageView
                    android:layout_width="@dimen/_75sdp"
                    android:layout_height="@dimen/_75sdp"
                    android:src="@drawable/seefoud" />
            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_width="@dimen/_100sdp"
                android:layout_height="wrap_content"
                android:text="Desserts Restaurants"
                android:textSize="@dimen/_14sdp"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="@dimen/_10sdp"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:layout_width="@dimen/_140sdp"
            android:layout_height="@dimen/_150sdp"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            app:cardCornerRadius="@dimen/_10sdp"
            app:cardElevation="@dimen/_5sdp"
            android:background="#fff">

            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_75sdp"
                app:cardElevation="@dimen/_5sdp"
                app:cardCornerRadius="@dimen/_5sdp"
                android:layout_gravity="center|top"
                android:layout_marginTop="@dimen/_5sdp">

                <ImageView
                    android:layout_width="@dimen/_75sdp"
                    android:layout_height="@dimen/_75sdp"
                    android:src="@drawable/seefoud" />

            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_width="@dimen/_100sdp"
                android:layout_height="wrap_content"
                android:text="Desserts Restaurants"
                android:textSize="@dimen/_14sdp"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="@dimen/_10sdp"/>

        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <LinearLayout
        android:layout_width="@dimen/_140sdp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_gravity="right"
        android:layout_marginTop="@dimen/_40sdp"
        android:layout_marginRight="@dimen/_5sdp">

        <androidx.cardview.widget.CardView
            android:id="@+id/Card_SeeFood"
            android:layout_width="@dimen/_140sdp"
            android:layout_height="@dimen/_150sdp"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            app:cardCornerRadius="@dimen/_10sdp"
            app:cardElevation="@dimen/_5sdp"
            android:background="#fff">

            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_75sdp"
                app:cardElevation="@dimen/_5sdp"
                app:cardCornerRadius="@dimen/_5sdp"
                android:layout_gravity="center|top"
                android:layout_marginTop="@dimen/_5sdp">

                <ImageView
                    android:layout_width="@dimen/_75sdp"
                    android:layout_height="@dimen/_75sdp"
                    android:src="@drawable/seefoud" />

            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_width="@dimen/_100sdp"
                android:layout_height="wrap_content"
                android:text="Seafood"
                android:textSize="@dimen/_14sdp"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="@dimen/_10sdp"/>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/Card_ReadyFood"
            android:layout_width="@dimen/_140sdp"
            android:layout_height="@dimen/_150sdp"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginBottom="@dimen/_10sdp"
            app:cardCornerRadius="@dimen/_10sdp"
            app:cardElevation="@dimen/_5sdp"
            android:background="#fff">

            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/_75sdp"
                android:layout_height="@dimen/_75sdp"
                app:cardElevation="@dimen/_5sdp"
                app:cardCornerRadius="@dimen/_5sdp"
                android:layout_gravity="center|top"
                android:layout_marginTop="@dimen/_5sdp">

                <ImageView
                    android:layout_width="@dimen/_75sdp"
                    android:layout_height="@dimen/_75sdp"
                    android:src="@drawable/seefoud" />

            </androidx.cardview.widget.CardView>

            <TextView
                android:layout_width="@dimen/_100sdp"
                android:layout_height="wrap_content"
                android:text="Ready Food"
                android:textSize="@dimen/_14sdp"
                android:textColor="#000"
                android:textAlignment="center"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="@dimen/_10sdp"/>
        </androidx.cardview.widget.CardView>

    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_40sdp"
        android:orientation="horizontal"
        android:layout_gravity="bottom"
        android:background="@drawable/fram_bottombar_design">

        <FrameLayout
            android:id="@+id/F_Foo3"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_gravity="left|center"
            android:layout_marginLeft="@dimen/_20sdp">
            <ImageView
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/ic_action_food"
                android:layout_gravity="center|top" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Food"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold"
                android:layout_gravity="center|bottom"/>
        </FrameLayout>

        <FrameLayout
            android:id="@+id/F_Type"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_gravity="center">

            <ImageView
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/ic_action_food"
                android:layout_gravity="center|top" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Types"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold"
                android:layout_gravity="center|bottom"/>
        </FrameLayout>


        <FrameLayout
            android:id="@+id/F_Profile"
            android:layout_width="@dimen/_40sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_gravity="right|center"
            android:layout_marginRight="@dimen/_20sdp">
            <ImageView
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/ic_action_account"
                android:layout_gravity="center|top" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Profile"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold"
                android:layout_gravity="center|bottom"/>
        </FrameLayout>
    </FrameLayout>
</FrameLayout>